@PACKAGE_INIT@

if(NOT HDF5_FOUND)
  message("Searching package HDF5...")
  ENABLE_LANGUAGE(C)
  find_package(HDF5 REQUIRED COMPONENTS C CXX HL)
  include_directories(${HDF5_INCLUDE_DIRS})
  link_directories(${HDF5_LIBRARIES} ${HDF5_HL_LIBRARIES})
endif(NOT HDF5_FOUND)

foreach(target @TARGETS_EXPORT_NAMES@)
    include("${CMAKE_CURRENT_LIST_DIR}/${target}.cmake")
    message("Including target ${target}")
endforeach()

check_required_components("@PROJECT_NAME@")

set(@PROJECT_NAME@_LIBRARY
      @LIBS_EXPORT_ALIASES@
)
set(@PROJECT_NAME@_LIBRARIES ${@PROJECT_NAME@_LIBRARY})
set(@PROJECT_NAME@_INCLUDE_DIR
    ${CMAKE_CURRENT_LIST_DIR}/../../../include
    ${GDAL_INCLUDE_DIR})

set(@PROJECT_NAME@_FOUND "TRUE")

